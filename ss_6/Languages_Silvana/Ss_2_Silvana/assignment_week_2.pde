//set parameters of canvas size
size(1080,720);

//changing the background with RGB values 0-255
background(255, 255,0);

//changing the fill color
fill(198, 244, 30);

//changing stroke color
stroke(0,0, 0);

//changing stroke weight
strokeWeight(5);

//set parameters of function with x,y coordinates for where to begin and end the line
//line(x1,y1,x2,y2);
line(20,30, 400,300);

//lets make another line
line(400, 300, 200,700);

line(200, 700, 400, 700);

//changing the fill color
fill(255, 255, 255);

//changing stroke wieght
strokeWeight(5);

//changing the stroke color
stroke(242,21, 15);

//rectMode()
rect(200,200, 200, 200);


//changing stroke wieght
strokeWeight(5);

//changing the stroke color
stroke(28,242,10);


//changing the fill color
fill(0, 0, 255);

//making a ellipse
ellipse(150, 150, 150,150);

//changing the fill color
fill(242,21,15);

//changing the stroke color
stroke(55, 37, 234);

//changing stroke weight
strokeWeight(4);

//lets make a rectangle
rect(400,300,200, 400,10);


//changing stroke weight
strokeWeight(3);

//changing the fill color
fill(28,242,10);

//making a triangle
//triangle(x1, y1, x2, y2, x3, y3);
triangle(600,400, 850,500, 600,600);